#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mattwang/Documents/personal_proj/portfolio/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.18.20_markdown-wasm@1.2.0/node_modules/contentlayer/bin/node_modules:/Users/mattwang/Documents/personal_proj/portfolio/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.18.20_markdown-wasm@1.2.0/node_modules/contentlayer/node_modules:/Users/mattwang/Documents/personal_proj/portfolio/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.18.20_markdown-wasm@1.2.0/node_modules:/Users/mattwang/Documents/personal_proj/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mattwang/Documents/personal_proj/portfolio/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.18.20_markdown-wasm@1.2.0/node_modules/contentlayer/bin/node_modules:/Users/mattwang/Documents/personal_proj/portfolio/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.18.20_markdown-wasm@1.2.0/node_modules/contentlayer/node_modules:/Users/mattwang/Documents/personal_proj/portfolio/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.18.20_markdown-wasm@1.2.0/node_modules:/Users/mattwang/Documents/personal_proj/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../contentlayer/bin/cli.cjs" "$@"
else
  exec node  "$basedir/../contentlayer/bin/cli.cjs" "$@"
fi
